from discord.ext import commands
import discord
import random
from discord.ext.commands import check
from discord.ext.commands import Bot
import asyncio
from discord.ext import commands

TOKEN = 'ODI5NzI2NDkwNzYwNDQ1OTUy.YG8VAg.QRuKOX6jJw62twzKos2UN62n3pM'
client = commands.Bot(command_prefix='.')


@client.event
async def on_ready():
    await client.change_presence(status=discord.Status.online, activity=discord.Game('Seu pai de calcinha'))
    print('O PAI TA ON')


@client.command()
async def ping(ctx):
    await ctx.send(f'pong! {round(client.latency*1000)}ms')


@client.command()
async def igor(ctx):
    resposta = ['MINHA CABE√áA DOI',
                'MEUS OLHOS',
                'MEUS JOELHOS']
    await ctx.channel.purge(limit=1)
    await ctx.send(f'{random.choice(resposta)}')


@client.command()
async def mamaki(ctx, member: discord.Member):
    await ctx.channel.purge(limit=1)
    await ctx.send(f'D√Å UMA SUGADA {member.mention}ü§Æ')


@client.command()
async def clear(ctx, amount=5):
    await ctx.channel.purge(limit=amount)


@client.command()
async def kick(ctx, member: discord.Member, *, reason=None):
    await member.kick(reason=reason)
    await ctx.channel.purge(limit=1)
    await ctx.send(f'{ctx.author.mention} botou {member.mention} pra mamar')


def in_voice_channel():
    def predicate(ctx):
        return ctx.author.voice and ctx.author.voice.channel
    return check(predicate)


@in_voice_channel()
@client.command()
async def move(ctx, *, channel: discord.VoiceChannel):
    for members in ctx.author.voice.channel.members:
        await members.move_to(channel)
    await ctx.channel.purge(limit=1)


@client.command()
async def bule(ctx, member: discord.Member):
    channel1 = ctx.guild.get_channel(695427648595165214)
    channel2 = ctx.author.voice.channel
    for i in range(10):
        await member.edit(voice_channel=channel1)
        await member.edit(voice_channel=channel2)


client.run(TOKEN)
